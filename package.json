        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^22.3.0",
        "@types/ws": "^8.5.12",
        "concurrently": "^8.2.2",
        "nodemon": "^3.1.4",
        "typescript": "^5.5.4"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.19.2",
        "kysely": "^0.27.4",
        "mysql2": "^3.11.0",
        "ws": "^8.18.0"
    }
